{
  "metadata": {
    "title": "Learn FB15K Data Set with QuatE^1 as described by Zhang et al., 2019."
  },
  "pipeline": {
    "dataset": "fb15k",
    "model": "QuatE",
    "model_kwargs": {
      "embedding_dim": 200,
      "entity_regularizer": "Lp",
      "entity_regularizer_kwargs": {
        "apply_only_once": false,
        "weight": 0.05,
        "p": 2.0,
        "normalize": false
      },
      "relation_regularizer": "Lp",
      "relation_regularizer_kwargs": {
        "apply_only_once": false,
        "weight": 0.05,
        "p": 2.0,
        "normalize": false
      }
    },
    "optimizer": "Adagrad",
    "optimizer_kwargs": {
      "lr": 0.1
    },
    "loss": "BCEWithLogitsLoss",
    "loss_kwargs": {
      "reduction": "mean"
    },
    "training_loop": "SLCWA",
    "training_kwargs": {
      "num_epochs": 5000,
      "batch_size": 9300
    },
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 10
    },
    "evaluator_kwargs": {
      "filtered": true
    }
  },
  "results": {
    "best": {
      "mean_rank": 41,
      "hits_at_k": {
        "1": 0.700,
        "3": 0.821,
        "10": 0.878
      },
      "mean_reciprocal_rank": 0.770
    }
  }
}