{
  "metadata": {
    "title": "Learn WN18 Data Set with TransH as described by Wang et al., 2014"
  },
  "pipeline": {
    "dataset": "wn18",
    "model": "TransH",
    "model_kwargs": {
      "embedding_dim": 50,
      "scoring_fct_norm": 2
    },
    "regularizer": "TransH",
    "regularizer_kwargs": {
      "weight": 0.25,
      "epsilon": 1e-05
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.01
    },
    "loss": "MarginRankingLoss",
    "loss_kwargs": {
      "reduction": "mean",
      "margin": 1
    },
    "training_loop": "SLCWA",
    "negative_sampler": "bernoulli",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 4800
    },
    "evaluator_kwargs": {
      "filtered": true
    }
  },
  "results": {
    "hits_at_k": {
      "unknown": {
        "10": 0.823
      }
    },
    "mean_rank": {
      "unknown": 388
    }
  }
}
