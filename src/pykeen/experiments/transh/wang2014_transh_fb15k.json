{
  "metadata": {
    "title": "Learn FB15K Dataset with TransH as described by Wang et al., 2014"
  },
  "pipeline": {
    "dataset": "fb15k",
    "model": "TransH",
    "model_kwargs": {
      "predict_with_sigmoid": false,
      "embedding_dim": 100,
      "scoring_fct_norm": 2,
      "entity_initializer": "xavier_uniform",
      "relation_initializer": "xavier_uniform"
    },
    "regularizer": "TransH",
    "regularizer_kwargs": {
      "weight": 1.0,
      "epsilon": 1e-05
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.005
    },
    "loss": "MarginRankingLoss",
    "loss_kwargs": {
      "reduction": "mean",
      "margin": 0.25
    },
    "training_loop": "SLCWA",
    "negative_sampler": "bernoulli",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 4800
    },
    "evaluator_kwargs": {
      "filtered": true
    }
  },
  "results": {
    "hits_at_k": {
      "unknown": {
        "10": 0.644
      }
    },
    "mean_rank": {
      "unknown": 87
    }
  }
}
