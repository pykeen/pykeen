{
  "metadata": {
    "title": "Learn WN18 Dataset with KG2E as described by He et al., 2015"
  },
  "pipeline": {
    "dataset": "wn18",
    "model": "KG2E",
    "model_kwargs": {
      "embedding_dim": 50,
      "c_min": 0.03,
      "c_max": 3.0,
      "entity_initializer": "uniform",
      "relation_initializer": "uniform",
      "entity_constrainer": "clamp_norm",
      "relation_constrainer": "clamp_norm"
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.01
    },
    "loss": "MarginRankingLoss",
    "loss_kwargs": {
      "reduction": "mean",
      "margin": 4.0
    },
    "training_loop": "SLCWA",
    "negative_sampler": "bernoulli",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 20
    },
    "evaluator_kwargs": {
      "filtered": true
    }
  },
  "results": {
    "hits_at_k": {
      "unknown": {
        "10": 0.928
      }
    },
    "mean_rank": {
      "unknown": 331
    }
  }
}