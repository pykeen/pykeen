{
  "metadata": {
    "title": "Learn FB15k Dataset with SimplE as described by Kazemi et al., 2018",
    "comments": "They make use of inverse relations. Normalization argument of regularizer is set to false. SimplE uses the optimistic rank definition for evaluation, cf. https://github.com/Mehran-k/SimplE/blob/29108230b63920afa38067b1aff8b8d53d07ed01/reader.py#L148."
  },
  "pipeline": {
    "dataset": "fb15k",
    "model": "SimplE",
    "model_kwargs": {
      "embedding_dim": 200,
      "clamp_score": 20.0,
      "entity_initializer": "uniform",
      "relation_initializer": "uniform"
    },
    "regularizer": "PowerSum",
    "regularizer_kwargs": {
      "apply_only_once": false,
      "weight": 0.1,
      "p": 2.0,
      "normalize": false
    },
    "optimizer": "Adagrad",
    "optimizer_kwargs": {
      "lr": 0.05
    },
    "loss": "SoftplusLoss",
    "loss_kwargs": {
      "reduction": "mean"
    },
    "training_loop": "SLCWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 10
    },
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 4831
    },
    "evaluator_kwargs": {
      "filtered": true
    }
  },
  "results": {
    "best": {
      "mean_reciprocal_rank": 0.727,
      "hits_at_k": {
        "1": 0.660,
        "3": 0.773,
        "10": 0.838
      }
    }
  }
}