{
  "metadata": {
    "title": "Learn FB15k-237 Dataset with R-GCN as described by Schlichtkrull et al., 2018",
    "comments": ""
  },
  "pipeline": {
    "dataset": "fb15k237",
    "model": "R-GCN",
    "model_kwargs": {
      "embedding_dim": 500,
      "decomposition": "block",
      "decomposition_kwargs": {
        "num_blocks": 100
      },
      "base_entity_initializer": "xavier_uniform",
      "relation_initializer": "xavier_uniform",
      "interaction": "distmult",
      "num_layers": 2,
      "self_loop_dropout": 0.2,
      "edge_dropout": 0.4,
      "regularizer": "Lp",
      "regularizer_kwargs": {
        "weight": 0.01
      }
    },
    "optimizer": "Adam",
    "optimizer_kwargs": {
      "lr": 0.1
    },
    "loss": "BCEWithLogits",
    "loss_kwargs": {
      "reduction": "mean"
    },
    "training_loop": "SLCWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 30000,
      "sampler": "schlichtkrull",
      "gradient_clipping_max_norm": 1.0
    },
    "evaluator_kwargs": {
      "filtered": true
    }
  },
  "results": {
    "mean_reciprocal_rank": {
      "worst": 0.248
    },
    "hits_at_k": {
      "worst": {
        "1": 0.153,
        "3": 0.258,
        "10": 0.414
      }
    }
  }
}