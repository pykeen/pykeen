{
  "metadata": {
    "title": "Learn FB15k Dataset with R-GCN as described by Schlichtkrull et al., 2018",
    "comments": ""
  },
  "pipeline": {
    "dataset": "fb15k",
    "model": "R-GCN",
    "model_kwargs": {
      "embedding_dim": 200,
      "decomposition": "bases",
      "decomposition_kwargs": {
        "num_bases": 2
      },
      "base_entity_initializer": "xavier_uniform",
      "relation_initializer": "xavier_uniform",
      "interaction": "distmult",
      "edge_dropout": 0.4,
      "self_loop_dropout": 0.2
    },
    "optimizer": "Adagrad",
    "optimizer_kwargs": {
      "lr": 0.1
    },
    "loss": "BCEWithLogits",
    "loss_kwargs": {
      "reduction": "mean"
    },
    "training_loop": "SLCWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 30000,
      "sampler": "schlichtkrull"
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "regularizer": "Lp",
    "regularizer_kwargs": {
      "apply_only_once": false,
      "weight": 0.01,
      "p": 2.0,
      "dim": null,
      "normalize": false
    }
  }
}