{
  "metadata": {
    "title": "Learn WN18 Data Set with ConvE as described by Dettmers et al., 2018"
  },
  "pipeline": {
    "dataset": "wn18",
    "model": "ConvE",
    "model_kwargs": {
      "embedding_dim": 200,
      "input_channels": 1,
      "output_channels": 32,
      "embedding_height": 10,
      "embedding_width": 20,
      "kernel_height": 3,
      "kernel_width": 3,
      "input_dropout": 0.2,
      "feature_map_dropout": 0.2,
      "output_dropout": 0.3
    },
    "optimizer": "Adam",
    "optimizer_kwargs": {
      "lr": 0.001
    },
    "loss": "BCEAfterSigmoidLoss",
    "loss_kwargs": {
      "reduction": "mean"
    },
    "training_loop": "LCWA",
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 128,
      "label_smoothing": 0.1
    },
    "evaluator_kwargs": {
      "filtered": true
    }
  }
}