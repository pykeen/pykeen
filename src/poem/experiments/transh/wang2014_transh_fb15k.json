{
  "metadata": {
    "title": "Learn FB15K Data Set with TransH as described by Wang et al., 2014"
  },
  "pipeline": {
    "data_set": "fb15k",
    "model": "TransH",
    "model_kwargs": {
      "embedding_dim": 100,
      "scoring_fct_norm": 2
    },
    "regularizer": "TransH",
    "regularizer_kwargs": {
      "weight": 1.0,
      "epsilon": 0.00001,
      "normalize": true
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.005
    },
    "loss": "MarginRankingLoss",
    "loss_kwargs": {
      "margin": 0.25
    },
    "training_loop": "OWA",
    "negative_sampler": "bernoulli",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 4800
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 16
    }
  }
}
