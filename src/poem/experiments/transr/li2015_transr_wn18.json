{
  "metadata": {
    "title": "Learn WN18 Data Set with TransR as described by Li et al., 2015"
  },
  "pipeline": {
    "dataset": "wn18",
    "model": "TransR",
    "model_kwargs": {
      "embedding_dim": 50,
      "relation_dim": 50,
      "scoring_fct_norm": 2
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.001
    },
    "loss": "MarginRankingLoss",
    "loss_kwargs": {
      "margin": 4
    },
    "training_loop": "OWA",
    "negative_sampler": "bernoulli",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 1440
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 16
    }
  }
}
