{
  "metadata": {
    "title": "Learn WN18 Data Set with RotatE as described by Sun et al., 2019",
    "comments": "Could not find initial learning rate; could not find number of epochs"
  },
  "pipeline": {
    "data_set": "wn18",
    "model": "RotatE",
    "model_kwargs": {
      "embedding_dim": 500
    },
    "optimizer": "Adam",
    "loss": "NegativeSamplingSelfAdversarialLoss",
    "loss_kwargs": {
      "adversarial_temperature": 0.5,
      "margin": 12
    },
    "training_loop": "OWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1024
    },
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 512,
      "sub_batch_size": 256
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 16
    }
  }
}

