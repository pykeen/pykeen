{
  "metadata": {
    "title": "Learn FB15K Data Set with TransE as described by Bordes et al., 2013",
    "comments": "Batch_size is not mentioned in the paper. We found out that this value works."
  },
  "pipeline": {
    "dataset": "fb15k",
    "model": "TransE",
    "model_kwargs": {
      "embedding_dim": 50,
      "scoring_fct_norm": 1
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 0.01
    },
    "loss": "MarginRankingLoss",
    "loss_kwargs": {
      "margin": 1
    },
    "training_loop": "OWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 32
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 128
    }
  }
}
