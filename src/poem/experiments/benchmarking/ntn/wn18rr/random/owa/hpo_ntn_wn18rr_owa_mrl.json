{
  "metadata": {
    "title": "HPO Over WN18RR for NTN"
  },
  "ablation": {
    "datasets": [
      "wn18rr"
    ],
    "models": [
      "NTN"
    ],
    "model_kwargs": {
      "NTN": {
        "num_slices": 4
      }
    },
    "model_kwargs_ranges": {
      "NTN": {
        "embedding_dim": {
          "type": "int",
          "low": 50,
          "high": 500,
          "q": 100
        }
      }
    },
    "training_loops": [
      "owa"
    ],
    "optimizers": [
      "adam",
      "adadelta"
    ],
    "optimizer_kwargs": {
      "NTN": {
        "adam": {
          "weight_decay": 0.0
        },
        "adadelta": {
          "weight_decay": 0.0
        }
      }
    },
    "optimizer_kwargs_ranges": {
      "NTN": {
        "adam": {
          "lr": {
            "type": "float",
            "low": 0.001,
            "high": 0.1,
            "scale": "log"
          }
        },
        "adadelta": {
          "lr": {
            "type": "float",
            "low": 0.001,
            "high": 0.1,
            "scale": "log"
          }
        }
      }
    },
    "loss_functions": [
      "MarginRankingLoss"
    ],
    "loss_kwargs": {
      "NTN": {
        "MarginRankingLoss": {}
      }
    },
    "loss_kwargs_ranges": {
      "NTN": {
        "MarginRankingLoss": {
          "margin": {
            "type": "float",
            "low": 0.5,
            "high": 10,
            "q": 1.0
          }
        }
      }
    },
    "regularizers": [
      "NoRegularizer"
    ],
    "regularizer_kwargs": {
      "NTN": {
        "NoRegularizer": {}
      }
    },
    "regularizer_kwargs_ranges": {
      "NTN": {
        "NoRegularizer": {}
      }
    },
    "negative_sampler": "BasicNegativeSampler",
    "negative_sampler_kwargs": {
      "NTN": {
        "BasicNegativeSampler": {}
      }
    },
    "negative_sampler_kwargs_ranges": {
      "NTN": {
        "BasicNegativeSampler": {
          "num_negs_per_pos": {
            "type": "int",
            "low": 1,
            "high": 50,
            "q": 5
          }
        }
      }
    },
    "create_inverse_triples": [
      false,
      true
    ],
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 32
    },
    "training_kwargs": {
      "NTN": {
        "owa": {
          "batch_size": 256,
          "sub_batch_size": 128,
          "label_smoothing": 0.0
        }
      }
    },
    "training_kwargs_ranges": {
      "NTN": {
        "owa": {
          "num_epochs": {
            "type": "int",
            "low": 500,
            "high": 2000,
            "q": 100
          }
        }
      }
    },
    "early_stopping": true,
    "early_stopping_kwargs": {
      "frequency": 50,
      "patience": 2,
      "delta": 0.002
    }
  },
  "optuna": {
    "n_trials": 100,
    "timeout": 86400,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop"
  }
}

