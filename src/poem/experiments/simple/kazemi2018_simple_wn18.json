{
  "metadata": {
    "title": "Learn WN18 Data Set with SimplE as described by Kazemi et al., 2018",
    "comments": "They make use of inverse relations. Normalization argument of regularizer is set to false."
  },
  "pipeline": {
    "data_set": "wn18",
    "model": "SimplE",
    "model_kwargs": {
      "embedding_dim": 200
    },
    "regularizer": "PowerSum",
    "regularizer_kwargs": {
      "weight": 0.03,
      "p": 2.0,
      "normalize": false
    },
    "optimizer": "Adagrad",
    "optimizer_kwargs": {
      "lr": 0.1
    },
    "loss": "SoftplusLoss",
    "training_loop": "OWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 1
    },
    "training_kwargs": {
      "num_epochs": 1000,
      "batch_size": 4831
    },
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": 16
    }
  }
}
